friendships = [(0,1,3),(0,2,3),(1,2,3),(1,3,3),(2,3,3),(3,4,3),
               (4,5,3),(5,6,3),(5,7,3),(6,8,3),(7,8,3),(8,9,3)]

print(type(friendships))

for i,j,w in friendships: 
    #isso funciona pq users[i] eh o usuario cuja id eh i
    print("i:" + str(i) + " j: " + str(j) + " w:" + str(w))

